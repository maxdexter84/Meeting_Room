<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextHeaderCalendar">
        <item name="fontFamily">@font/roboto</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black_020303</item>
        <item name="android:textSize">@dimen/dimens_16_sp</item>
        <item name="android:letterSpacing">0.03</item>
    </style>

    <style name="DayOfWeekCalendar">
        <item name="fontFamily">@font/roboto</item>
        <item name="android:textSize">@dimen/dimens_12_sp</item>
        <item name="android:letterSpacing">0.03</item>
        <item name="android:textColor">@color/dark_gray</item>
    </style>

    <style name="DateTextCalendar">
        <item name="fontFamily">@font/roboto</item>
        <item name="android:textSize">@dimen/dimens_12_sp</item>
        <item name="android:letterSpacing">0.03</item>
        <item name="android:textColor">@color/black_020303</item>
    </style>

    <style name="RoomMaxCapacity">
        <item name="fontFamily">@font/roboto</item>
        <item name="android:textSize">@dimen/dimens_14_sp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">@color/black_020303</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">@dimen/dimens_0_dp</item>
        <item name="android:layout_height">@dimen/dimens_1_dp</item>
        <item name="android:layout_marginTop">@dimen/dimens_16_dp</item>
        <item name="android:background">@color/color_for_popup_window_divider</item>
    </style>
</resources>