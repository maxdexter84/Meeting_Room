<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressButton">
        <attr name="text" format="string"/>
        <attr name="state" format="enum">
            <enum name="disabled" value="0" />
            <enum name="enabled" value="1" />
            <enum name="loading" value="2" />
        </attr>
    </declare-styleable>
</resources>